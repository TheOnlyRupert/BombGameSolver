<Page x:Class="BombGameSolver.Source.Modules.WhoFirstModule"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      mc:Ignorable="d">
    <Grid Background="DarkSlateGray">
        <Grid.RowDefinitions>
            <RowDefinition Height="3*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <ScrollViewer Grid.Row="0" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>

                <StackPanel Grid.Column="0" Margin="20 5 5 5">
                    <TextBox IsEnabled="False" MinWidth="50" Text="NULL" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="6" FontSize="20" HorizontalContentAlignment="Center"
                             Margin="0 0 0 5" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="BLANK" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="5" FontSize="20" HorizontalContentAlignment="Center"
                             Margin="0 0 0 5" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="C" FontSize="20" HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="4" FontSize="20" HorizontalContentAlignment="Center"
                             Margin="0 0 0 5" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="CEE" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="6" FontSize="20" HorizontalContentAlignment="Center"
                             Margin="0 0 0 5" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="DISPLAY" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="6" FontSize="20" HorizontalContentAlignment="Center"
                             Margin="0 0 0 5" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="FIRST" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="4" FontSize="20" HorizontalContentAlignment="Center"
                             Margin="0 0 0 5" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="HOLD ON" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="6" FontSize="20" HorizontalContentAlignment="Center" />
                </StackPanel>
                <StackPanel Grid.Column="1" Margin="0 5 5 5">
                    <TextBox IsEnabled="False" MinWidth="50" Text="LEAD" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="6" FontSize="20" HorizontalContentAlignment="Center"
                             Margin="0 0 0 5" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="LED" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="2" FontSize="20" HorizontalContentAlignment="Center"
                             Margin="0 0 0 5" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="LEED" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="3" FontSize="20" HorizontalContentAlignment="Center"
                             Margin="0 0 0 5" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="NO" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="6" FontSize="20" HorizontalContentAlignment="Center"
                             Margin="0 0 0 5" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="NOTHING" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="2" FontSize="20" HorizontalContentAlignment="Center"
                             Margin="0 0 0 5" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="OKAY" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="4" FontSize="20" HorizontalContentAlignment="Center"
                             Margin="0 0 0 5" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="READ" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="5" FontSize="20" HorizontalContentAlignment="Center" />
                </StackPanel>
                <StackPanel Grid.Column="2" Margin="0 5 5 5">
                    <TextBox IsEnabled="False" MinWidth="50" Text="RED" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="5" FontSize="20" HorizontalContentAlignment="Center"
                             Margin="0 0 0 5" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="REED" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="3" FontSize="20" HorizontalContentAlignment="Center"
                             Margin="0 0 0 5" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="SAYS" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="6" FontSize="20" HorizontalContentAlignment="Center"
                             Margin="0 0 0 5" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="SEE" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="6" FontSize="20" HorizontalContentAlignment="Center"
                             Margin="0 0 0 5" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="THEIR" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="5" FontSize="20" HorizontalContentAlignment="Center"
                             Margin="0 0 0 5" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="THERE" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="6" FontSize="20" HorizontalContentAlignment="Center"
                             Margin="0 0 0 5" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="THEY'RE" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="2" FontSize="20" HorizontalContentAlignment="Center" />

                </StackPanel>
                <StackPanel Grid.Column="3" Margin="0 5 5 5">
                    <TextBox IsEnabled="False" MinWidth="50" Text="THEY ARE" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="3" FontSize="20" HorizontalContentAlignment="Center"
                             Margin="0 0 0 5" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="UR" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="1" FontSize="20" HorizontalContentAlignment="Center"
                             Margin="0 0 0 5" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="YES" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="2" FontSize="20" HorizontalContentAlignment="Center"
                             Margin="0 0 0 5" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="YOU" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="5" FontSize="20" HorizontalContentAlignment="Center"
                             Margin="0 0 0 5" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="YOUR" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="6" FontSize="20" HorizontalContentAlignment="Center"
                             Margin="0 0 0 5" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="YOU'RE" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="5" FontSize="20" HorizontalContentAlignment="Center"
                             Margin="0 0 0 5" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="YOU ARE" FontSize="20"
                             HorizontalContentAlignment="Center" />
                    <TextBox IsEnabled="False" MinWidth="50" Text="5" FontSize="20" HorizontalContentAlignment="Center" />
                </StackPanel>

                <StackPanel Grid.Column="4" Margin="5 5 5 0">
                    <Button Content="BLANK" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="DONE" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="FIRST" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="HOLD" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="LEFT" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="LIKE" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="MIDDLE" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="NEXT" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="NO" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="NOTHING" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="OKAY" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="PRESS" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="READY" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="RIGHT" FontSize="20" HorizontalContentAlignment="Center" />
                </StackPanel>
                <StackPanel Grid.Column="5" Margin="0 5 5 0">
                    <Button Content="SURE" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="U" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="UR" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="UHHH" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="UH UH" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="UH HUH" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="WAIT" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="WHAT" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="WHAT?" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="YES" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="YOU" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="YOUR" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="YOU'RE" FontSize="20" HorizontalContentAlignment="Center" />
                    <Button Content="YOU ARE" FontSize="20" HorizontalContentAlignment="Center" />
                </StackPanel>
            </Grid>
        </ScrollViewer>
        <TextBox Text="RIGHT, MIDDLE, LEFT, WHAT?, UH HUH, UHHH, SURE, YES, NO, YOU, YOUR" Grid.Row="1" FontSize="40"
                 TextWrapping="Wrap" Background="Transparent" Foreground="White" MinWidth="150"
                 TextAlignment="Left" VerticalAlignment="Center" Margin="30 0 30 0" MinHeight="110" IsEnabled="False" />
    </Grid>
</Page>