        <TextBlock Background="CadetBlue" TextAlignment="Center" FontSize="40" Text="THING" Grid.Column="3"
                   Focusable="False"
                   Grid.Row="5" />
        <TextBlock Background="CadetBlue" TextAlignment="Center" FontSize="40" Text="THINK" Grid.Column="3"
                   Focusable="False"
                   Grid.Row="6" />
        <TextBlock Background="CadetBlue" TextAlignment="Center" FontSize="40" Text="THREE" Grid.Column="4"
                   Focusable="False"
                   Grid.Row="0" />
        <TextBlock Background="Aqua" TextAlignment="Center" FontSize="40" Text="WATER" Grid.Column="4" Grid.Row="1"
                   Focusable="False" />
        <TextBlock Background="Aqua" TextAlignment="Center" FontSize="40" Text="WHERE" Grid.Column="4" Grid.Row="2"
                   Focusable="False" />
        <TextBlock Background="Aqua" TextAlignment="Center" FontSize="40" Text="WHICH" Grid.Column="4" Grid.Row="3"
                   Focusable="False" />
        <TextBlock Background="Aqua" TextAlignment="Center" FontSize="40" Text="WORLD" Grid.Column="4" Grid.Row="4"
                   Focusable="False" />
        <TextBlock Background="Aqua" TextAlignment="Center" FontSize="40" Text="WOULD" Grid.Column="4" Grid.Row="5"
                   Focusable="False" />
        <TextBlock Background="Aqua" TextAlignment="Center" FontSize="40" Text="WRITE" Grid.Column="4" Grid.Row="6"
                   Focusable="False" />
    </Grid>
</Page>