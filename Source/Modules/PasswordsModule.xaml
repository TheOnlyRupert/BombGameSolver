                   Focusable="False"
                   Grid.Row="4" />
        <TextBlock Background="Aqua" TextAlignment="Center" FontSize="40" Text="NEVER" Grid.Column="1" Grid.Row="5"
                   Focusable="False" />
        <TextBlock Background="CadetBlue" TextAlignment="Center" FontSize="40" Text="OTHER" Grid.Column="1"
                   Focusable="False"
                   Grid.Row="6" />
        <TextBlock Background="Aqua" TextAlignment="Center" FontSize="40" Text="PLACE" Grid.Column="2" Grid.Row="0"
                   Focusable="False" />
        <TextBlock Background="Aqua" TextAlignment="Center" FontSize="40" Text="PLANT" Grid.Column="2" Grid.Row="1"
                   Focusable="False" />
        <TextBlock Background="Aqua" TextAlignment="Center" FontSize="40" Text="POINT" Grid.Column="2" Grid.Row="2"
                   Focusable="False" />
        <TextBlock Background="CadetBlue" TextAlignment="Center" FontSize="40" Text="RIGHT" Grid.Column="2"
                   Focusable="False"
                   Grid.Row="3" />
        <TextBlock Background="Aqua" TextAlignment="Center" FontSize="40" Text="SMALL" Grid.Column="2" Grid.Row="4"
                   Focusable="False" />
        <TextBlock Background="Aqua" TextAlignment="Center" FontSize="40" Text="SOUND" Grid.Column="2" Grid.Row="5"
                   Focusable="False" />
        <TextBlock Background="Aqua" TextAlignment="Center" FontSize="40" Text="SPELL" Grid.Column="2" Grid.Row="6"
                   Focusable="False" />
        <TextBlock Background="Aqua" TextAlignment="Center" FontSize="40" Text="STILL" Grid.Column="3" Grid.Row="0"
                   Focusable="False" />
        <TextBlock Background="Aqua" TextAlignment="Center" FontSize="40" Text="STUDY" Grid.Column="3" Grid.Row="1"
                   Focusable="False" />
        <TextBlock Background="CadetBlue" TextAlignment="Center" FontSize="40" Text="THEIR" Grid.Column="3"
                   Focusable="False"
                   Grid.Row="2" />
