<Page x:Class="BombGameSolver.Source.Views.Solver"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:views="clr-namespace:BombGameSolver.Source.Views"
      mc:Ignorable="d"
      d:DataContext="{d:DesignInstance views:Solver}">
    <Grid>
        <Border>
            <Border.Background>
                <ImageBrush x:Name="BackgroundImage" TileMode="None" ViewportUnits="RelativeToBoundingBox"
                            ImageSource="../../Resources/background.png" />
            </Border.Background>

            <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto">
                <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" TextBlock.TextAlignment="Center">
                    <Border Background="White" CornerRadius="10" Padding="30 40 30 40" Width="640" Height="600"
                            Margin="50 50 50 50">
                        <Grid>

                            <!-- Initial Bomb Settings -->
                            <StackPanel Name="BombSetupView">
                                <TextBlock Text="Initial Bomb Setup" FontSize="48" Margin="0 0 0 30" />

                                <!-- Checkboxes -->
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>

                                    <StackPanel Orientation="Horizontal" Grid.Column="0">
                                        <CheckBox Name="IsCaRon">
                                            <CheckBox.LayoutTransform>
                                                <ScaleTransform ScaleX="2" ScaleY="2" />
                                            </CheckBox.LayoutTransform>
                                        </CheckBox>

                                        <TextBlock Text=" Lit CAR" FontSize="20" />
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Grid.Column="1">
                                        <CheckBox Name="IsFrKon" Margin="2 2 2 2">
                                            <CheckBox.LayoutTransform>
                                                <ScaleTransform ScaleX="2" ScaleY="2" />
                                            </CheckBox.LayoutTransform>
                                        </CheckBox>
                                        <TextBlock Text=" Lit FRK" FontSize="20" />
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Grid.Column="2">
                                        <CheckBox Name="HasPp" Margin="2 2 2 2">
                                            <CheckBox.LayoutTransform>
                                                <ScaleTransform ScaleX="2" ScaleY="2" />
                                            </CheckBox.LayoutTransform>
                                        </CheckBox>
                                        <TextBlock Text=" Parallel Port" FontSize="20" />
                                    </StackPanel>
                                </Grid>

                                <TextBlock Text="Battery Amount" TextAlignment="Left" FontSize="20" Margin="0 20 0 5" />
                                <DockPanel VerticalAlignment="Center" Width="150" HorizontalAlignment="Left">
                                    <TextBox
                                        Text="{Binding ElementName=BatterySlider, Path=Value,
                                        UpdateSourceTrigger=PropertyChanged}"
                                        DockPanel.Dock="Right" Margin="10 0 0 0" FontSize="20" Width="30"
                                        TextAlignment="Center" />
                                    <Slider Name="BatterySlider" Minimum="0" Maximum="3" TickFrequency="1"
                                            TickPlacement="BottomRight" IsSnapToTickEnabled="True" FontSize="40" />
                                </DockPanel>

                                <TextBlock Text="Serial Number" FontSize="20" HorizontalAlignment="Left"
                                           Margin="0 20 0 5" />
                                <TextBox Name="SerialInput" Width="150" TextChanged="SerialInput_OnTextChanged"
                                         Padding="4" TextAlignment="Left" MaxLength="6" FontSize="20"
                                         HorizontalAlignment="Left" />

                                <Button Content="Continue" Name="InitialContinueButton" Margin="20" Padding="8"
                                        FontSize="20"
                                        IsEnabled="False" Click="InitialContinueButton_OnClick"
                                        HorizontalAlignment="Right" Width="150" />

                            </StackPanel>

                            <!-- Main Bomb Modules -->
                            <StackPanel Name="MainButtonsView" Visibility="Hidden">
                                <TextBlock Text="Main Modules" FontSize="48" Margin="0 0 0 30" />

                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>

                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="*" />
                                        <RowDefinition Height="*" />
                                        <RowDefinition Height="*" />
                                        <RowDefinition Height="*" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>

                                    <Button FontSize="20" Height="60" Content="Normal Wires"
                                            Click="NormalWiresButton_OnClick" Grid.Column="0" Grid.Row="0" />
                                    <Button FontSize="20" Height="60" Content="Colored Button"
                                            Click="ColoredWiresButton_OnClick" Grid.Column="0" Grid.Row="1" />
                                    <Button FontSize="20" Height="60" Content="Simon Says"
                                            Click="SimonSaysButton_OnClick" Grid.Column="0" Grid.Row="2" />
                                    <Button FontSize="20" Height="60" Content="Symbol Keypads"
                                            Click="KeypadButton_OnClick" Grid.Column="0" Grid.Row="3" />
                                    <Button FontSize="20" Height="60" Content="Who's on First"
                                            Click="WhoOnFirstButton_OnClick" Grid.Column="1" Grid.Row="0" />
                                    <Button FontSize="20" Height="60" Content="Memory" Click="MemoryButton_OnClick"
                                            Grid.Column="1" Grid.Row="1" />
                                    <Button FontSize="20" Height="60" Content="Morse Code"
                                            Click="MorseCodeButton_OnClick" Grid.Column="1" Grid.Row="2" />
                                    <Button FontSize="20" Height="60" Content="Christmas Wires"
                                            Click="ComplicatedWiresButton_OnClick" Grid.Column="1" Grid.Row="3" />
                                    <Button FontSize="20" Height="60" Content="Wire Sequence"
                                            Click="WireSquButton_OnClick" Grid.Column="2" Grid.Row="0" />
                                    <Button FontSize="20" Height="60" Content="Maze" Click="MazeButton_OnClick"
                                            Grid.Column="2" Grid.Row="1" />
                                    <Button FontSize="20" Height="60" Content="Passwords"
                                            Click="PasswordButton_OnClick" Grid.Column="2" Grid.Row="2" />
                                    <Button FontSize="20" Height="60" Content="Positional Knob"
                                            Click="PosKnob_OnClick" Grid.Column="2" Grid.Row="3" />
                                    <Button FontSize="20" Height="60" Content="- Back to Setup -"
                                            Click="BackSetupButton_OnClick" Grid.Column="2" Grid.Row="4" />
                                </Grid>
                            </StackPanel>

                            <!-- Normal Wires View -->
                            <StackPanel Name="NormalWiresView" Visibility="Hidden">
                                <TextBlock Text="Normal Wires" FontSize="48" Margin="0 0 0 30" />

                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>

                                    <Button Name="NormalWiresBlackButton" FontSize="20" Foreground="White"
                                            Background="Black" Click="NormalWiresBlackButton_OnClick"
                                            Content="Black" Grid.Column="0" />
                                    <Button Name="NormalWiresBlueButton" FontSize="20" Foreground="White"
                                            Background="Blue" Click="NormalWiresBlueButton_OnClick"
                                            Content="Blue" Grid.Column="1" />
                                    <Button Name="NormalWiresRedButton" FontSize="20" Foreground="White"
                                            Background="Red" Click="NormalWiresRedButton_OnClick"
                                            Content="Red" Grid.Column="2" />
                                    <Button Name="NormalWiresWhiteButton" FontSize="20" Foreground="Black"
                                            Background="White" Click="NormalWiresWhiteButton_OnClick"
                                            Content="White" Grid.Column="3" />
                                    <Button Name="NormalWiresYellowButton" FontSize="20" Foreground="Black"
                                            Background="Yellow" Click="NormalWiresYellowButton_OnClick"
                                            Content="Yellow" Grid.Column="4" />
                                </Grid>

                                <StackPanel Orientation="Horizontal" Margin="0 10 0 10">
                                    <TextBlock Text="Wire Order" FontSize="16" Margin="0 0 5 0" />
                                    <TextBox Name="NormalWiresOrderTextbox" IsEnabled="False" Width="360" FontSize="16"
                                             TextAlignment="Left" />
                                </StackPanel>

                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Text="Wire Total" FontSize="16" Margin="0 0 10 0" />
                                    <TextBox Text="0" FontSize="16" Name="NormalWiresTotalTextbox" IsEnabled="False"
                                             Width="30" />
                                </StackPanel>

                                <!-- Button Group -->
                                <StackPanel Orientation="Horizontal" Margin="0 20 0 20">
                                    <Button Content="Back" Click="InitialContinueButton_OnClick" Width="100"
                                            HorizontalAlignment="Right" FontSize="20" />
                                    <Button Content="Reset All" Width="100" Click="NormalWires_ClearButton_OnClick"
                                            HorizontalAlignment="Right" FontSize="20" />
                                    <Button Name="NormalWiresConfirmButton" Click="NormalWiresConfirmButton_OnClick"
                                            IsEnabled="False" Content="Confirm" Width="100" HorizontalAlignment="Right"
                                            FontSize="20" />
                                </StackPanel>

                                <TextBox Name="NormalWiresOutputTextBox" IsEnabled="False" FontSize="48"
                                         VerticalContentAlignment="Center" />
                            </StackPanel>

                            <!-- Colored Button View -->
                            <StackPanel Name="ColoredButtonView" Visibility="Hidden">
                                <TextBlock Text="Colored Button" FontSize="48" Margin="0 0 0 30" />

                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="*" />
                                    </Grid.ColumnDefinitions>

                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="*" />
                                        <RowDefinition Height="*" />
                                        <RowDefinition Height="*" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>

                                    <Button FontSize="20" Content="Blue" Name="ColoredButtonBlueButton"
                                            Foreground="White" Background="Blue"
                                            Click="ColoredButtonBlueButton_OnClick" Grid.Column="0" Grid.Row="0" />
                                    <Button FontSize="20" Content="Red" Name="ColoredButtonRedButton"
                                            Foreground="White" Background="Red" Click="ColoredButtonRedButton_OnClick"
                                            Grid.Column="0" Grid.Row="1" />
                                    <Button FontSize="20" Content="White" Name="ColoredButtonWhiteButton"
                                            Foreground="Black" Background="White"
                                            Click="ColoredButtonWhiteButton_OnClick" Grid.Column="0" Grid.Row="2" />
                                    <Button FontSize="20" Content="Yellow" Name="ColoredButtonYellowButton"
                                            Foreground="Black" Background="Yellow"
                                            Click="ColoredButtonYellowButton_OnClick" Grid.Column="0" Grid.Row="3" />

                                    <Button FontSize="20" Content="ABORT" Name="ColoredButtonAbortButton"
                                            Click="ColoredButtonAbortButton_OnClick" Grid.Column="1" Grid.Row="0" />
                                    <Button FontSize="20" Content="DETONATE" Name="ColoredButtonDetonateButton"
                                            Click="ColoredButtonDetonateButton_OnClick" Grid.Column="1" Grid.Row="1" />
                                    <Button FontSize="20" Content="HOLD" Name="ColoredButtonHoldButton" Grid.Column="1"
                                            Click="ColoredButtonHoldButton_OnClick" Grid.Row="2" />
                                    <Button FontSize="20" Content="PRESS" Name="ColoredButtonPressButton"
                                            Grid.Column="1" Click="ColoredButtonPressButton_OnClick" Grid.Row="3" />
                                </Grid>

                                <StackPanel Orientation="Horizontal" Margin="0 10 0 10">
                                    <TextBox Name="ColoredButtonColorTextbox" IsEnabled="False" Width="80"
                                             FontSize="16" Margin="0 0 20 0" />
                                    <TextBox Name="ColoredButtonTextTextbox" IsEnabled="False" Width="80" FontSize="16" />
                                </StackPanel>

                                <!-- Button Group -->
                                <StackPanel Orientation="Horizontal" Margin="0 20 0 20">
                                    <Button Content="Back" Click="InitialContinueButton_OnClick" Width="100"
                                            HorizontalAlignment="Right" FontSize="20" />
                                    <Button Content="Reset All" Width="100" Click="ColoredButton_ClearButton_OnClick"
                                            HorizontalAlignment="Right" FontSize="20" />
                                </StackPanel>

                                <TextBox Name="ColoredButtonOutputTextBox" IsEnabled="False" FontSize="48"
                                         VerticalContentAlignment="Center" />
                                <TextBlock FontSize="32" Text="Blue: 4   Yellow: 5   Other: 1"
                                           HorizontalAlignment="Left" Margin="10 20 0 0" />

                            </StackPanel>

                            <!-- Simon Says View -->
                            <StackPanel Name="SimonSaysView" Visibility="Hidden">
                                <TextBlock Text="Simon Says" FontSize="48" Margin="0 0 0 30" />

                                <TextBlock Text="Number of Strikes" FontSize="16" TextAlignment="Left" />
                                <DockPanel VerticalAlignment="Center" Width="200" HorizontalAlignment="Left"
                                           Margin="0 5 0 20">
                                    <TextBox
                                        Text="{Binding ElementName=StrikeSlider, Path=Value,
                                        UpdateSourceTrigger=PropertyChanged}"
                                        DockPanel.Dock="Right" Margin="10 0 0 0"
                                        FontSize="20" Width="30" TextAlignment="Center" />
                                    <Slider Name="StrikeSlider" Minimum="0" Maximum="2" TickFrequency="1"
                                            ValueChanged="StrikeSlider_OnValueChanged"
                                            TickPlacement="BottomRight" IsSnapToTickEnabled="True" FontSize="40" />
                                </DockPanel>

                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*" />
                                        <ColumnDefinition Width="2*" />
                                        <ColumnDefinition Width="2*" />
                                        <ColumnDefinition Width="2*" />
                                        <ColumnDefinition Width="2*" />
                                    </Grid.ColumnDefinitions>

                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="*" />
                                        <RowDefinition Height="*" />
                                    </Grid.RowDefinitions>

                                    <TextBox Text="Old" IsEnabled="False" Grid.Column="0" Grid.Row="0" FontSize="20" />
                                    <TextBox Text="New" IsEnabled="False" Grid.Column="0" Grid.Row="1" FontSize="20" />

                                    <TextBox Text="Blue" Foreground="White" Background="Blue" IsEnabled="False"
                                             Grid.Column="1" Grid.Row="0" FontSize="20" />
                                    <TextBox Name="SimonSaysBlueTextbox" IsEnabled="False" Grid.Column="1" Grid.Row="1"
                                             FontSize="20" />

                                    <TextBox Text="Green" Foreground="White" Background="Green" IsEnabled="False"
                                             Grid.Column="2" Grid.Row="0" FontSize="20" />
                                    <TextBox Name="SimonSaysGreenTextbox" IsEnabled="False" Grid.Column="2"
                                             Grid.Row="1" FontSize="20" />

                                    <TextBox Text="Red" Foreground="White" Background="Red" IsEnabled="False"
                                             Grid.Column="3" Grid.Row="0" FontSize="20" />
                                    <TextBox Name="SimonSaysRedTextbox" IsEnabled="False" Grid.Column="3" Grid.Row="1"
                                             FontSize="20" />

                                    <TextBox Text="Yellow" Foreground="Black" Background="Yellow" IsEnabled="False"
                                             Grid.Column="4" Grid.Row="0" FontSize="20" />
                                    <TextBox Name="SimonSaysYellowTextbox" IsEnabled="False" Grid.Column="4"
                                             Grid.Row="1" FontSize="20" />
                                </Grid>

                                <Button Content="Back" Click="InitialContinueButton_OnClick" Width="100"
                                        Margin="0 20 0 0" FontSize="20"
                                        HorizontalAlignment="Left" />
                            </StackPanel>

                            <!-- Who's on First -->
                            <StackPanel Name="WhoOnFirstView" Visibility="Hidden">
                                <TextBlock Text="Who's on First?" FontSize="48" Margin="0 0 0 30" />

                                <StackPanel Name="WhoOnFirstPage1View">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>

                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                        </Grid.RowDefinitions>

                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="NULL ( 3 )"
                                                   FontSize="20" Grid.Column="0" Grid.Row="0" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="BLANK ( 5 )"
                                                   FontSize="20" Grid.Column="0" Grid.Row="1" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="C ( 4 )"
                                                   FontSize="20" Grid.Column="0" Grid.Row="2" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="CEE ( 6 )"
                                                   FontSize="20" Grid.Column="0" Grid.Row="3" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="DISPLAY ( 6 )"
                                                   FontSize="20" Grid.Column="0" Grid.Row="4" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="FIRST ( 4 )"
                                                   FontSize="20" Grid.Column="0" Grid.Row="5" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="HOLD ON ( 6 )"
                                                   FontSize="20" Grid.Column="0" Grid.Row="6" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="LEAD ( 6 )"
                                                   FontSize="20" Grid.Column="1" Grid.Row="0" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="LED ( 2 )"
                                                   FontSize="20" Grid.Column="1" Grid.Row="1" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="LEED ( 3 )"
                                                   FontSize="20" Grid.Column="1" Grid.Row="2" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="NO ( 6 )"
                                                   FontSize="20" Grid.Column="1" Grid.Row="3" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="NOTHING ( 2 )"
                                                   FontSize="20" Grid.Column="1" Grid.Row="4" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="OKAY ( 4 )"
                                                   FontSize="20" Grid.Column="1" Grid.Row="5" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="READ ( 5 )"
                                                   FontSize="20" Grid.Column="1" Grid.Row="6" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="RED ( 5 )"
                                                   FontSize="20" Grid.Column="2" Grid.Row="0" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="REED ( 3 )"
                                                   FontSize="20" Grid.Column="2" Grid.Row="1" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="SAYS ( 6 )"
                                                   FontSize="20" Grid.Column="2" Grid.Row="2" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="SEE ( 6 )"
                                                   FontSize="20" Grid.Column="2" Grid.Row="3" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="THEIR ( 5 )"
                                                   FontSize="20" Grid.Column="2" Grid.Row="4" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="THERE ( 6 )"
                                                   FontSize="20" Grid.Column="2" Grid.Row="5" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="THEY ARE ( 2 )"
                                                   FontSize="20" Grid.Column="2" Grid.Row="6" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="THEY'RE ( 3 )"
                                                   FontSize="20" Grid.Column="3" Grid.Row="0" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="UR ( 1 )"
                                                   FontSize="20" Grid.Column="3" Grid.Row="1" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="YES ( 2 )"
                                                   FontSize="20" Grid.Column="3" Grid.Row="2" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="YOU ( 5 )"
                                                   FontSize="20" Grid.Column="3" Grid.Row="3" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="YOU ARE ( 6 )"
                                                   FontSize="20" Grid.Column="3" Grid.Row="4" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="YOUR ( 5 )"
                                                   FontSize="20" Grid.Column="3" Grid.Row="5" />
                                        <TextBlock Background="LightBlue" TextAlignment="Left" Text="YOU'RE ( 5 )"
                                                   FontSize="20" Grid.Column="3" Grid.Row="6" />
                                    </Grid>
                                </StackPanel>

                                <StackPanel Name="WhoOnFirstPage2View" Visibility="Collapsed">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>

                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                            <RowDefinition Height="*" />
                                        </Grid.RowDefinitions>

                                        <Button Content="BLANK" Click="WhoOnFirstButton01_OnClick" FontSize="18"
                                                Grid.Column="0" Grid.Row="0" />
                                        <Button Content="DONE" Click="WhoOnFirstButton02_OnClick" FontSize="18"
                                                Grid.Column="0" Grid.Row="1" />
                                        <Button Content="FIRST" Click="WhoOnFirstButton03_OnClick" FontSize="18"
                                                Grid.Column="0" Grid.Row="2" />
                                        <Button Content="HOLD" Click="WhoOnFirstButton04_OnClick" FontSize="18"
                                                Grid.Column="0" Grid.Row="3" />
                                        <Button Content="LEFT" Click="WhoOnFirstButton05_OnClick" FontSize="18"
                                                Grid.Column="0" Grid.Row="4" />
                                        <Button Content="LIKE" Click="WhoOnFirstButton06_OnClick" FontSize="18"
                                                Grid.Column="0" Grid.Row="5" />
                                        <Button Content="MIDDLE" Click="WhoOnFirstButton07_OnClick" FontSize="18"
                                                Grid.Column="0" Grid.Row="6" />
                                        <Button Content="NEXT" Click="WhoOnFirstButton08_OnClick" FontSize="18"
                                                Grid.Column="1" Grid.Row="0" />
                                        <Button Content="NO" Click="WhoOnFirstButton09_OnClick" FontSize="18"
                                                Grid.Column="1" Grid.Row="1" />
                                        <Button Content="NOTHING" Click="WhoOnFirstButton10_OnClick" FontSize="18"
                                                Grid.Column="1" Grid.Row="2" />
                                        <Button Content="OKAY" Click="WhoOnFirstButton11_OnClick" FontSize="18"
                                                Grid.Column="1" Grid.Row="3" />
                                        <Button Content="PRESS" Click="WhoOnFirstButton12_OnClick" FontSize="18"
                                                Grid.Column="1" Grid.Row="4" />
                                        <Button Content="READY" Click="WhoOnFirstButton13_OnClick" FontSize="18"
                                                Grid.Column="1" Grid.Row="5" />
                                        <Button Content="RIGHT" Click="WhoOnFirstButton14_OnClick" FontSize="18"
                                                Grid.Column="1" Grid.Row="6" />
                                        <Button Content="SURE" Click="WhoOnFirstButton15_OnClick" FontSize="18"
                                                Grid.Column="2" Grid.Row="0" />
                                        <Button Content="U" Click="WhoOnFirstButton16_OnClick" FontSize="18"
                                                Grid.Column="2" Grid.Row="1" />
                                        <Button Content="UH HUH" Click="WhoOnFirstButton17_OnClick" FontSize="18"
                                                Grid.Column="2" Grid.Row="2" />
                                        <Button Content="UH UH" Click="WhoOnFirstButton18_OnClick" FontSize="18"
                                                Grid.Column="2" Grid.Row="3" />
                                        <Button Content="UHHH" Click="WhoOnFirstButton19_OnClick" FontSize="18"
                                                Grid.Column="2" Grid.Row="4" />
                                        <Button Content="UR" Click="WhoOnFirstButton20_OnClick" FontSize="18"
                                                Grid.Column="2" Grid.Row="5" />
                                        <Button Content="WAIT" Click="WhoOnFirstButton21_OnClick" FontSize="18"
                                                Grid.Column="2" Grid.Row="6" />
                                        <Button Content="WHAT" Click="WhoOnFirstButton22_OnClick" FontSize="18"
                                                Grid.Column="3" Grid.Row="0" />
                                        <Button Content="WHAT?" Click="WhoOnFirstButton23_OnClick" FontSize="18"
                                                Grid.Column="3" Grid.Row="1" />
                                        <Button Content="YES" Click="WhoOnFirstButton24_OnClick" FontSize="18"
                                                Grid.Column="3" Grid.Row="2" />
                                        <Button Content="YOU" Click="WhoOnFirstButton25_OnClick" FontSize="18"
                                                Grid.Column="3" Grid.Row="3" />
                                        <Button Content="YOU ARE" Click="WhoOnFirstButton26_OnClick" FontSize="18"
                                                Grid.Column="3" Grid.Row="4" />
                                        <Button Content="YOUR" Click="WhoOnFirstButton27_OnClick" FontSize="18"
                                                Grid.Column="3" Grid.Row="5" />
                                        <Button Content="YOU'RE" Click="WhoOnFirstButton28_OnClick" FontSize="18"
                                                Grid.Column="3" Grid.Row="6" />
                                    </Grid>

                                </StackPanel>

                                <!-- Button Group -->
                                <StackPanel Orientation="Horizontal" Margin="0 20 0 20">
                                    <Button Content="Back" Click="InitialContinueButton_OnClick" Width="100"
                                            FontSize="20"
                                            HorizontalAlignment="Right" />
                                    <Button Content="Switch View" Width="120" Click="WhoOnFirstSwitchView_OnClick"
                                            FontSize="20"
                                            HorizontalAlignment="Right" />
                                </StackPanel>

                                <TextBox Visibility="Hidden" FontSize="16" Name="WhoOnFirstOutput1TextBox"
                                         HorizontalAlignment="Left" Background="LightGray" IsEnabled="False"
                                         Width="100" />
                                <TextBox Visibility="Hidden" TextWrapping="Wrap" Name="WhoOnFirstOutput2TextBox"
                                         IsEnabled="False" TextAlignment="Left" FontSize="24" />
                            </StackPanel>


                            <!-- Memory -->
                            <StackPanel Name="MemoryView" Visibility="Hidden">
                                <TextBlock Text="Memory" FontSize="48" Margin="0 0 0 30" />

                                <TextBox Name="MemoryCurrentRoundTextBox" Text="0% Done" Width="110" IsEnabled="False"
                                         Margin="0 0 0 -30" FontSize="20" HorizontalAlignment="Left" />

                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="10 0 0 10">
                                    <Button Name="MemoryInput1Button" Click="MemoryInput1Button_OnClick" Content="1"
                                            FontSize="32" Width="60" />
                                    <Button Name="MemoryInput2Button" Click="MemoryInput2Button_OnClick" Content="2"
                                            FontSize="32" Width="60" />
                                    <Button Name="MemoryInput3Button" Click="MemoryInput3Button_OnClick" Content="3"
                                            FontSize="32" Width="60" />
                                    <Button Name="MemoryInput4Button" Click="MemoryInput4Button_OnClick" Content="4"
                                            FontSize="32" Width="60" />
                                </StackPanel>

                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
